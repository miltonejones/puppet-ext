{
  "name": "Puppeteer Studio Chrome Extension",
  "version": "0.1.4",
  "description": "Run Puppeteer Studio Tests in The Browser", 
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "html2canvas.min.js", "jquery.min.js", "data.js", "index.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": ["http://*/*", "https://*/*", "activeTab"],
  "manifest_version": 2
}
